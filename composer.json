{
  "name": "kuaukutsu/test-funpay",
  "description": "Test: FunPay",
  "keywords": [
    "php",
    "test",
    "mysqli"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitriy Krivopalov",
      "email": "dimarik82@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/kuaukutsu/test-funpay/issues",
    "source": "https://github.com/kuaukutsu/test-funpay"
  },
  "require": {
    "php": "^8.3",
    "ext-mysqli": "*"
  },
  "require-dev": {
    "vimeo/psalm": "^5.15",
    "squizlabs/php_codesniffer": "^3.7",
    "roave/infection-static-analysis-plugin": "^1.3",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "platform": {
      "php": "8.3"
    },
    "allow-plugins": {
      "infection/extension-installer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "FpDbTest\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FpDbTest\\tests\\": "tests"
    }
  }
}
